   "metadata": {},
   "source": [
    "# Method 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "c510bcf5",
   "metadata": {},
   "outputs": [],
   "source": [
    "def adder(seq):\n",
    "    return sum(seq)"
